<h1 class="session-header"> Sign in to Lunchin': </h1>
<div class="login-bg">
  <form class="" action="<%= session_url %>" method="post">
    <%= render "shared/auth_token" %>
    <%= render "shared/email_password" %>

    <input type="submit" class="btn btn-default pull-right" value="Sign In!">
  </form>
</div>
