<ul class="review-author-info">
  <li>
    <img src="
      <% if (review.get('author'.photo_url)) {%>
        <%= review.get('author').photo_url %>
      <% } else { %>
        <%= 'https://s3-us-west-1.amazonaws.com/lunchin/assets/user_60_square.png' %>
      <% } %>
    " >
  </li>
  <li><%= review.get('author').first_name %></li>
</ul>

<div class="review-rating-body">
  <img class="small-rating-image" src="https://s3-us-west-1.amazonaws.com/lunchin/assets/<%= review.escape('rating') %>stars.png">
  <br>
  <%= review.escape('body') %>
</div>
