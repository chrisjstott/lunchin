<div class='panel panel-default'>
<div class='row panel-body'>
  <div class='col-md-3'>
    <img class='img-rounded review-user-pic' src="
      <% if (review.get('author'.photo_url)) {%>
        <%= review.get('author').photo_url %>
      <% } else { %>
        <%= 'https://s3-us-west-1.amazonaws.com/lunchin/assets/user_60_square.png' %>
      <% } %>
    " >
    <ul class='review-user-info'>
      <li><strong><%= review.get('author').first_name + ' ' + review.get('author').last_name[0] %>.</strong></li>
      <li><%= review.get('author').location %></li>
    </ul>
  </div>
  <div class='col-md-9'>
    <img class="small-rating-image" src="https://s3-us-west-1.amazonaws.com/lunchin/assets/<%= review.escape('rating') %>stars.png">
    <br>
    <%= review.escape('body') %>
  </div>
</div>
</div>